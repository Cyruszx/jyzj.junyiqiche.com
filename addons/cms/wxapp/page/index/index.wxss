.filterbar {
    position: relative;
    z-index: 1000
}

.filterbar__bd {
    display: -ms-flexbox;
    display: flex;
    position: relative;
    padding: 0 10rpx;
    background-color: #fff
}

.filterbar__bd:after {
    content: " ";
    position: absolute;
    left: 0;
    bottom: 0;
    right: 0;
    height: 2rpx;
    border-bottom: 2rpx solid #d9d9d9;
    color: #d9d9d9;
    transform-origin: 0 100%;
    transform: scaleY(.5)
}

.filterbar__item {
    position: relative;
    display: block;
    -ms-flex: 1;
    flex: 1;
    padding: 26rpx 0;
    text-align: center;
    font-size: 30rpx
}


