.filterbar__item--checked {
    color: #f15353
}

.filterbar__icon {
    width: 30rpx;
    height: 24rpx;
    margin-left: 6rpx;
    display: inline-block;
    background-repeat: no-repeat;
    background-position: center center;
    background-size: contain
}

.filterbar__icon--checkbox,
.filterbar__icon--radio {
    background-image: url(data:image/png;base64,)
}

.filterbar__icon--sort {
    background-position: 0 -52rpx;
    background-size: 18rpx 72rpx;
    background-image: url(data:image/png;base64,)
}

.filterbar__icon--sort-desc {
    background-position: 0 4rpx
}

.filterbar__icon--sort-asc {
    background-position: 0 -24rpx
}

.filterbar__icon--filter {
    background-image: url(data:image/png;base64,)
}

.filterbar__select {
    /* position: absolute; */
    /* top: 100rpx; */
    left: 0;
    z-index: 1010;
    width: 100%
}

.filterbar__pop {
    height: 100%;
    width: 90%;
    overflow: hidden;
    position: fixed;
    top: 0;
    left: 10%;
    z-index: 1010;
    background-color: #fff;
    box-sizing: border-box
}

.filterbar__scroll-view {
    height: 85%;
    height: calc(100% - 88rpx)
}

.filterbar__panel {
    padding: 0 30rpx
}

.filterbar__panel:last-child {
    padding-bottom: 40rpx
}

.filterbar__panel-hd {
    padding-top: 40rpx;
    padding-bottom: 4rpx;
    color: #252525;
    font-size: 30rpx;
    overflow-x: hidden;
    display: -ms-flexbox;
    display: flex;
    -ms-flex-align: center;
    align-items: center
}

.filterbar__panel-selected {
    width: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
    -ms-flex: 1;
    flex: 1;
    text-align: right;
    font-size: 26rpx;
    line-height: 48rpx;
    color: #f23030
}

.filterbar__groups {
    overflow: hidden;
    font-size: 26rpx;
    margin: 0 -10rpx
}

.select {
    background: #fff;
    padding: 0 20rpx 20rpx;
    box-sizing: border-box;
}

.select-brand {
    background-color: #f8f8f8;
    padding: 0;
}

.slider {
    margin: 30rpx;
}

.filterbar__group {
    width: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
    position: relative;
    height: 64rpx;
    width: 33.3333%;
    color: #232326;
    margin-top: 20rpx;
    float: left;
    box-sizing: border-box;
    padding-left: 10rpx;
    padding-right: 10rpx
}

.filterbar__btn,
.filterbar__check,
.filterbar__radio {
    width: auto;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    word-wrap: normal;
    height: 64rpx;
    display: block;
    text-align: center;
    line-height: 64rpx;
    background-color: #f0f2f5;
    border-radius: 10rpx;
    border: 2rpx solid #f0f2f5;
    box-sizing: border-box
}

.filterbar__check,
.filterbar__radio {
    position: absolute;
    left: 10rpx;
    top: 0;
    width: 100%;
    width: calc(100% - 20rpx);
    opacity: 0
}

.filterbar__btn--checked {
    border-color: #f23030;
    background-color: #fff;
    color: #f23030
}

.filterbar__btns {
    height: 88rpx;
    display: -webkit-box;
    overflow: hidden
}

.filterbar__btns .filterbar__btn {
    background-color: #fff;
    color: #252525;
    font-size: 32rpx;
    position: relative;
    display: -webkit-box;
    -webkit-box-flex: 1;
    -webkit-box-align: center;
    -webkit-box-pack: center;
    border: none;
    border-radius: 0;
    height: 88rpx;
    line-height: 88rpx
}

.filterbar__btns .filterbar__btn:before {
    content: " ";
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    height: 2rpx;
    border-top: 2rpx solid #d9d9d9;
    color: #d9d9d9;
    transform-origin: 0 0;
    transform: scaleY(.5)
}

.filterbar__btns .filterbar__btn--danger {
    background-color: #f23030;
    color: #fff
}

.filterbar__btns .filterbar__btn--danger:before {
    display: none
}


