}

.car:after {
    content: " ";
    position: absolute;
    left: 20rpx;
    bottom: 0;
    right: 20rpx;
    height: 1px;
    border-bottom: 1px solid #D5D5D6;
    color: #D5D5D6;
    transform-origin: 0 0;
    transform: scaleY(0.5);
}

.car:last-child:after {
    display: none;
}

.car__scroll {
    width: auto;
    overflow-x: auto;
    white-space: nowrap;
}

.car__image {
