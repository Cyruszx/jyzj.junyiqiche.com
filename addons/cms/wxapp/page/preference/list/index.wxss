.scroll-view {
    height: 260rpx;
    display: block;
    overflow-x: auto;
    white-space: nowrap
}

.scroll-view .car__box {
    display: inline-flex;
}

.scroll-view .car__more {
    transform: translateY(-30%);
    display: inline-flex;
    width: 300rpx;
}

.page {
    /* width: 750rpx; */
    display: flex;
    flex-direction: column;
    box-sizing: border-box;
    /* background-color: #EFEFF4; */
    font-size: 16px;
    font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.page .page_hd {
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    padding: 10rpx 20rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: row;
    color: #fff;
    box-sizing: border-box;
    z-index: 10;
    transition: background-color .3s;
}

.page .page_hd--fixed {
    background-color: #fff;
    color: #333;
}

.page .page_hd .page_hd_position {
    box-sizing: border-box;
    /*width: 16%;*/
    margin-right: 20rpx;
    height: 60rpx;
    line-height: 60rpx;
    position: relative;
    /*top: -2px;*/
}


/* .page .page_hd .page_hd_position .fa-angle-down {
    position: absolute;
    right: 30rpx;
    top: -5rpx;
} */

.page .page_hd .page_hd_search {
    display: flex;
    align-items: center;
    justify-content: center;
    /*width: 68%;*/
    flex: 1;
    height: 60rpx;
    border-radius: 30rpx;
    background-color: #f2f2f2;
    color: #888;
    text-align: center;
    box-sizing: border-box;
    font-size: 26rpx;
}

.page .page_hd .page_hd_search icon {
    vertical-align: middle;
    margin-top: -4rpx;
}

.page .page_hd .page_hd_phone {
    display: flex;
    align-items: center;
    justify-content: center;
    /*width: 16%;*/
    height: 100%;
    text-align: center;
    position: relative;
    padding-left: 10rpx;
    box-sizing: border-box;
    font-size: 40rpx;
}

.button-clear {
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    padding: 0 10rpx;
    min-width: 72rpx;
}

.button-text {
    font-size: 20rpx;
    margin-top: 4rpx;
    /*letter-spacing: -4rpx;*/
}


/* .page .page_hd .page_hd_phone .fa-volume-control-phone {
    position: absolute;
    top: -1rpx;
} */

.page .page_broadcast {
    /* height: 290rpx; */
}

.page .newcar_optimization {
    margin-top: 15rpx;
    position: relative;
    align-items: center;
}

.page .newcar_optimization .fa-chevron-right {
    position: absolute;
    right: 200rpx;
    top: 12rpx;
    z-index: 20;
    color: #888;
}


/* 轮播 */

.swiperClass {
    height: 360rpx;
    margin: 0;
    padding: 0;
    /* background: #EFEFF4; */
}

.slide-image {
    width: 100%;
    /* height: 300rpx; */
    height: 100%;
    /* border-radius: 10px; */
    position: relative;
}

.brandPlan {
    padding: 20rpx;
    background-color: #fff;
}

.brandPlan__media {
    background-color: #f2f2f2;
    padding: 20rpx 10rpx !important;
    margin-top: 20rpx;
}

.brandPlan__title {
    color: #333;
    font-size: 28rpx;
    text-align: left;
    font-family: PingFangSC-regular;
    margin-top: 6rpx;
}

.brandPlan__title--payment {
    font-size: 24rpx;
    color: rgba(229, 28, 35, 1);
    font-weight: bold;
}

.brandPlan__title--monthly {
    font-size: 24rpx;
    color: #696969
}

.brandPlan__address {
    font-size: 20rpx;
    position: absolute;
    right: 10rpx;
    bottom: 6rpx;
}

.brandPlan__arrow {
    position: absolute;
    left: 0;
    top: 50%;
    display: block;
    width: 46rpx;
    height: 250rpx;
    transform: translate3d(-100%, -50%, 0);
    background-size: contain;
    background-image: url("data:image/png;base64,");
}


